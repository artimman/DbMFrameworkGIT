{
    "name": "artimman/dbmframework",
    "description": "The DbM Framework PHP MVC Simple CMS",
    "keywords": ["dbmframework", "microframework", "php framework", "version cms"],
    "homepage": "https://github.com/artimman/dbmframework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Design by Malina, Arthur Malinowski",
            "homepage": "https://dbm.org.pl"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "phpmailer/phpmailer": "^6.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.45",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "classmap": [
            "application/"
        ],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
